    "davefeedread": "^0.5.11",
    "daveutils": "^0.4.49",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "googleapis": "^51.0.0",
    "helmet": "^3.21.2",
    "http-errors": "^1.7.3",
    "is-image-url": "^1.1.8",
    "jade": "^1.11.0",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
